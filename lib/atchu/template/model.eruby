class <%= model.class_name %> < ActiveRecord::Base
  <% unless model.class_name == model.table_name %>
    <%= "self.table_name = '#{model.table_name}'" -%>
  <% end %>

  <% unless "id" == model.primary_key || model.primary_key.nil? %>
    <%= "self.primary_key = #{model.primary_key}" -%>
  <% end %>

end
